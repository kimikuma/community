<div class="container">
  <div class="row">
    <div class="col-md-5 my-auto">
<h1>Create Group</h1>
<%=form_with model: @group do |f| %>
<p>グループ名</p>
<%=f.text_field:name %>
<p>紹介文</p>
<%=f.text_area:introduction %>
<p>グループ画像</p>
<%=f.file_field:group_image,accepct: "image/*" %>
<p><%=f.submit "Create Group",class: "mt-3" %></p>
<% end %>
</div>
</div>
</div>